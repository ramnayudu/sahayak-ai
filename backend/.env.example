# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account-key.json

# Firebase Configuration
FIREBASE_SERVICE_ACCOUNT_KEY={"type": "service_account", "project_id": "..."}

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_DEFAULT_MODEL=gemma:7b

# API Configuration
PORT=8000
DEBUG=true

# Database Configuration (if using PostgreSQL for additional data)
DATABASE_URL=postgresql://user:password@localhost/sahayak_ai

# Redis Configuration (for caching and task queue)
REDIS_URL=redis://localhost:6379

# Security
JWT_SECRET_KEY=your-super-secret-jwt-key
CORS_ORIGINS=http://localhost:3000,https://sahayak-ai.web.app
